# Career Mart Backend Environment Variables
# Copy this file to .env and fill in your values

# Node Environment
NODE_ENV=production

# Server Configuration
PORT=5000

# MongoDB Configuration
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/career-mart

# API Configuration (for cron scraper)
API_BASE_URL=https://your-backend-url.onrender.com/api/scrape

# Scraping Configuration
SCRAPING_INTERVAL=0 */6 * * *
SCRAPING_KEYWORDS=Software Engineer, Developer, Engineer
SCRAPING_LOCATION=Remote, New York, San Francisco
SCRAPING_TYPE=Full-Time
SCRAPING_LIMIT=50

# Optional: Add any other environment variables you need 